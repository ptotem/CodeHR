<div class="row-fluid">
  <div class="span6">
    <div class="row-fluid">
      <div class="table-header">Report</div>
      <div class="span6">
        Report Date :- <%= @dy_report.report_date %>
      </div>
      <div class="span6">
        Reviewed By :- <%= @dy_report.reviewed_by %>
      </div>
    </div>
  </div>
</div>

<div class="row-fluid">
  <div class="span6">
    <table id="report_table" class="table table-striped table-bordered table-hover">
      <thead>
      <tr>
        <th colspan="3">Detail</th>
        <!--<th></th>-->
        <!--<th></th>-->
      </tr>
      </thead>
      <tbody>
        <tr>
          <td colspan="3">
            <%= @dy_report.object_hash %>
          </td>
        </tr>
        <tr>
          <td colspan="3">
            <%= @dy_report.object_hash.size %>
          </td>
        </tr>
        <% @dy_report.object_hash.each_with_index do |obj_hash, index| %>
          <tr>
            <td><%= obj_hash[0] %></td>
            <td><%= obj_hash[1] %></td>
            <td><%= obj_hash[2] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>