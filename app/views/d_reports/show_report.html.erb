<div class="row-fluid">
  <div class="span6">
    <div class="row-fluid">
      <div class="table-header">Report</div>
      <div class="span6">
        Report Date :- <%= @dy_report.report_date rescue "" %>
      </div>
      <div class="span6">
        Reviewed By :- <%= @dy_report.reviewed_by rescue "" %>
      </div>
    </div>
  </div>
</div>

<div class="row-fluid">
  <div class="span6">
    <table id="report_table" class="table table-striped table-bordered table-hover">
      <thead>
      <tr>
        <th>Detail</th>
        <th>Detail</th>
        <th>Detail</th>
      </tr>
      </thead>
      <tbody>
      <% if !@dy_report.nil? %>
        <% @dy_report.object_hash.to_a.each_with_index do |obj_hash, index| %>
          <tr>
            <td><%= obj_hash.rows rescue "" %></td>
          </tr>
        <% end %>
      <% end %>
      </tbody>
    </table>
  </div>
</div>

<br>
<br>
<%= @dy_report.object_hash.to_json.to_a[0] rescue nil %>
