<div class="row-fluid">
  <div class="span4">
    <%= link_to 'New Group Master', new_group_master_path, :class=>"btn btn-info" %>
  </div>
</div>

<br>

<div class="row-fluid">
  <div class="span12">
    <div class="table-header">Listing Group Masters</div>
    <table id="group_master_table" class="table table-striped table-bordered table-hover">
      <thead>
          <tr>
            <th>Group Code</th>
            <th>Group Name</th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
      </thead>

      <tbody>
          <% @group_masters.each do |group_master| %>
              <tr>
                <td><%= group_master.group_code %></td>
                <td><%= group_master.group_name %></td>
                <td><%= link_to 'Show', group_master %></td>
                <td><%= link_to 'Edit', edit_group_master_path(group_master) %></td>
                <td><%= link_to 'Destroy', group_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>

<%= javascript_include_tag "jquery.dataTables.min" %>
<%= javascript_include_tag "jquery.dataTables.bootstrap" %>
<%= javascript_include_tag "custom_dataTable" %>


<script>
    $(function(){
        apply_dataTable('#group_master_table', '#group_master_table_wrapper','#group_master_table_length', '#group_master_table_filter');
    });
</script>
