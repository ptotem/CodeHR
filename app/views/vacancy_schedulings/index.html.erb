<div class="row-fluid">
  <div class="span4">
    <%= link_to 'New Vacancy Scheduling', new_vacancy_scheduling_path, :class=>"btn btn-info" %>
  </div>
</div>

<br>

<div class="row-fluid">
  <div class="span12">
    <div class="table-header">Listing Vacancy Schedulings</div>
    <table id="vacancy_scheduling_table" class="table table-striped table-bordered table-hover">
      <thead>
          <tr>
            <th></th>
            <th></th>
            <th></th>
          </tr>
      </thead>

      <tbody>
          <% @vacancy_schedulings.each do |vacancy_scheduling| %>
              <tr>
                <td><%= link_to 'Show', vacancy_scheduling %></td>
                <td><%= link_to 'Edit', edit_vacancy_scheduling_path(vacancy_scheduling) %></td>
                <td><%= link_to 'Destroy', vacancy_scheduling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>

<%= javascript_include_tag "jquery.dataTables.min" %>
<%= javascript_include_tag "jquery.dataTables.bootstrap" %>
<%= javascript_include_tag "custom_dataTable" %>


<script>
    $(function(){
        apply_dataTable('#vacancy_scheduling_table', '#vacancy_scheduling_table_wrapper','#vacancy_scheduling_table_length', '#vacancy_scheduling_table_filter');
    });
</script>
