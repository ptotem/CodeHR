<h1>Listing Process Masters</h1>

<div class="row-fluid">
  <div class="offset1 span8">
    <table class="table table-striped table-bordered table-hover">
      <thead>
          <tr>
            <th>Name</th>
            <th></th>
            <th></th>
            <th></th>
          </tr>
      </thead>

      <tbody>
          <% @process_masters.each do |process_master| %>
              <tr>
                <td><%= process_master.name %></td>
                <td><%= link_to 'Show', process_master %></td>
                <td><%= link_to 'Edit', edit_process_master_path(process_master) %></td>
                <td><%= link_to 'Destroy', process_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
          <% end %>
      </tbody>
    </table>
  </div>
</div>

<br />

<%= link_to 'New Process master', new_process_master_path %>
