                    <%= master_step.hidden_field :sequence, :class=>"my_seq" %>

                    <div class="span4">
                      <div class="control-group">
                        <!--<label class="control-label">Action</label>-->
                        <label>Action</label>
                        <div class="controls drpdwn" style="margin-left: 0;">
