    function show_this_attr(this_obj){
        setTimeout(function() {
            var this_href, this_href_id;
            this_href = $(this_obj).attr('href');
            this_href_id = this_href.replace("#","");

            $('.tabbable').find('.tab-content').find('.tab-pane').each(function(i,v){
                console.log("this tab id :- " + $(v).attr('id'));
                $(v).removeClass('active');
                $(v).css('display','none');
                if ( $(v).attr('id') == this_href_id ) {
                    $(v).addClass('active');
                    $(v).css('display','block');
                }
            });
        }, 1000);
    }

    function set_step_seq(){
        $('#sortable').find('li').each(function(i,v){
            var this_li = $(v).find('a').attr('href');
            $(this_li).find('.my_seq').val($(v).index());
        });
    }

