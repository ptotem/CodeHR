<%= nested_form_for(@master_pro, html: {class: "form-horizontal"}) do |f| %>
    <% if @master_pro.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@master_pro.errors.count, "error") %> prohibited this master_pro from being saved:</h2>

          <ul>
            <% @master_pro.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div class="row-fluid">
      <div class="span6">
        <div class="control-group">
          <label class="control-label" style="text-align: left;">Name</label>
          <div class="controls">
            <%= f.text_field :name, :style=>"width: 80%;" %>
          </div>
        </div>
      </div>
    </div>

    <hr>
    <div class="page-header position-relative">
      <h3>Master Steps</h3>
    </div>


    <div class="row-fluid tabs-row">
