<%#= link_to 'New Left panel', new_left_panel_path %>

<div class="row-fluid">
  <div class="span12">
    <div class="table-header">Listing left_panels</div>
    <table id="group_master_table" class="table table-striped table-bordered table-hover">
      <thead>
      <tr>
        <th>Name</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
      </thead>

      <tbody>
      <% @left_panels.each do |left_panel| %>
          <tr>
            <td><%= left_panel.name %></td>
            <td><%= link_to 'Show', left_panel %></td>
            <td><%= link_to 'Edit', edit_left_panel_path(left_panel) %></td>
            <td><%= link_to 'Destroy', left_panel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    <%= link_to 'New Left panel', new_left_panel_path %>
  </div>
</div>

<%= javascript_include_tag "jquery.dataTables.min" %>
<%= javascript_include_tag "jquery.dataTables.zbootstrap" %>
<%= javascript_include_tag "custom_dataTable" %>


<script>
    $(function(){
        apply_dataTable('#group_master_table', '#group_master_table_wrapper','#group_master_table_length', '#group_master_table_filter');
    });
</script>